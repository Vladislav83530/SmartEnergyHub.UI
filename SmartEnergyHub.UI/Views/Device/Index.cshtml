@using System.Security.Claims;

@section Scripts {
    <script type="text/javascript" src="/js/device.js" asp-append-version="true"></script>
}

@section Styles {
    <link href="/css/device.css" rel="stylesheet" asp-append-version="true" />
}


<div id="main-device-block" class="container mt-5" data-userid="@User.Claims.First(c => c.Type == ClaimTypes.NameIdentifier).Value">
    <div class="row">
        <div class="col-8">
            <div class="row">
                <div class="col-12"></div>
            </div>
            <div class="row">
                <div id="device-block" class="col-12"></div>
            </div>
        </div>
        <div id="residence-block" class="col-4 container-residence">
        </div>
    </div>
</div>